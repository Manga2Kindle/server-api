{
  "name": "server-api",
  "version": "2.0.0",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.compile.json -w",
    "start": "npm run tsc:w & nodemon -w dist dist/index.js",
    "start:prod": "npm run tsc && cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "typeorm": "tsed typeorm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manga2Kindle/server-api.git"
  },
  "keywords": [
    "manga",
    "kindle",
    "e-reader"
  ],
  "author": {
    "name": "Eduardo Fernandez",
    "email": "yo@edufdez.es"
  },
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/Manga2Kindle/server-api/issues"
  },
  "homepage": "https://github.com/Manga2Kindle/server-api#readme",
  "dependencies": {
    "@tsed/ajv": "6.1.5",
    "@tsed/common": "6.1.5",
    "@tsed/core": "6.1.5",
    "@tsed/di": "6.1.5",
    "@tsed/exceptions": "6.1.5",
    "@tsed/json-mapper": "6.1.5",
    "@tsed/platform-express": "6.1.5",
    "@tsed/schema": "6.1.5",
    "@tsed/swagger": "6.1.5",
    "@tsed/typeorm": "6.1.5",
    "ajv": "6.12.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "mariadb": "2.5.1",
    "method-override": "3.0.0",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.28"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "2.0.0",
    "@tsed/cli-plugin-typeorm": "2.0.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/method-override": "0.0.31",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "concurrently": "5.3.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "readme": "./README.md",
  "_id": "server-api@2.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "description": "./README.md"
}
